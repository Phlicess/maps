#points_gbif{
  polygon-fill: #CC0000;
  polygon-opacity: 1;
}
#points_gbif{
  [count <= 10] { polygon-fill: #FFFF00;  }
  [count <= 100] { polygon-fill: #FFCC00;  }
  [count <= 1000] { polygon-fill: #FF9900;  }
  [count <= 10000] { polygon-fill: #FF6600;  }
  [count <= 100000] { polygon-fill: #FF3300;  }
}
