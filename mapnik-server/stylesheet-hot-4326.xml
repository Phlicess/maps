<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map srs="+proj=longlat +datum=WGS84 +no_defs">

  <Style name="occurrence" filter-mode="first" image-filters="colorize-alpha(#009900,#00ff00,#ffffff)">
    <Rule>
      <DotSymbolizer width="3" opacity="0.5" fill="#cc0000" />
    </Rule>
  </Style>
  <Layer name="occurrence" srs="+proj=longlat +datum=WGS84 +no_defs">
    <StyleName>occurrence</StyleName>
  </Layer>

</Map>
