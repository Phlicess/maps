    <Rule>
      <Filter>([count] &lt;= 10000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#E0A1E6" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 1000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#DF9EE5" opacity="1" width="2" allow-overlap="true" placement="point"/>
