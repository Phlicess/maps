      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#CC0000" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 100000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#ff3300" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 10000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#ff6600" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 1000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#ff9900" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 100)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#ffcc00" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
