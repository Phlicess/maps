<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=merc +a=6378137 +b=6378137 +lat_ts=0.0 +lon_0=0.0 +x_0=0.0 +y_0=0.0 +k=1.0 +units=m +nadgrids=@null +wktext +no_defs +over" background-color="#00000000" maximum-extent="-20037508.34,-20037508.34,20037508.34,20037508.34">

    <!-- Does not work, since we've already pixilated in processing -->
  <Style name="points" image-filters="colorize-alpha(#000000, #00FF00, #FFFFFF)" >
    <Rule>
      <MarkersSymbolizer marker-type="ellipse" fill="#FF0000" opacity="0.25" width="3" allow-overlap="true" placement="point"/>
    </Rule>
  </Style>

  <Style name="heat" image-filters="colorize-alpha(blue, cyan, green, yellow , orange, red)" >
