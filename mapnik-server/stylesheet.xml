      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#FFFFFF" opacity="1" width="2" allow-overlap="true" placement="point"/>
    </Rule>
    <Rule>
      <Filter>([count] &lt;= 100000)</Filter>
      <MarkersSymbolizer marker-type="ellipse" stroke-width="0" fill="#EDC8F1" opacity="1" width="2" allow-overlap="true" placement="point"/>
